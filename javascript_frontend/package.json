{
  "name": "@local/javascript_frontend",
  "version": "0.0.2",
  "private": false,
  "scripts": {
    "serve": "npx --no parcel serve --dist-dir tmp public/index.html",
    "build:js_css": "npx --no parcel build --no-source-maps --dist-dir dist/static src/index.mjs",
    "__build:css": "npx --no parcel build --no-source-maps --no-content-hash --dist-dir dist/static src/index.scss",
    "__build:html": "npx --no parcel build --no-source-maps --no-content-hash --dist-dir dist/page public/index.html --public-url=./",
    "build-clean": "npx --no shx touch dist && npx --no shx rm -r dist",
    "build-rename": "npx --no shx mv dist/static/index.js dist/static/wingechr_javascript_frontend.min.js && npx --no shx mv dist/static/index.css dist/static/wingechr_javascript_frontend.min.css",
    "build": "npm run build-clean && npx --no npm-run-all build:* && npm run build-rename",
    "publish": "npm publish --access public",
    "_serve": "npx --no react-scripts start",
    "_build": "npx --no react-scripts build",
    "_test": "npx --no react-scripts test",
    "_eject": "npx --no react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@local/javascript_package": "file:../javascript_package",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "suggestedDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react-scripts": "5.0.1",
    "@craco/craco": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.7",
    "@parcel/packager-raw-url": "^2.13.3",
    "@parcel/transformer-babel": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "@parcel/transformer-webmanifest": "^2.12.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "shx": "^0.3.4",
    "svgo": "^3.3.2"
  }
}
