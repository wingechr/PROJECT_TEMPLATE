{
  "name": "@wingechr/javascript_frontend",
  "version": "0.0.2",
  "private": false,
  "scripts": {
    "_serve": "npx --no parcel serve --dist-dir tmp public/index.html",
    "_build:js": "npx --no parcel build --no-source-maps --dist-dir build/static src/index.mjs && npx shx mv build/static/index.js build/static/wingechr_javascript_frontend.min.js",
    "_build:css": "npx --no parcel build --no-source-maps --no-content-hash --dist-dir build/static src/index.scss && shx mv build/static/index.css build/static/wingechr_javascript_frontend.min.css",
    "_build:html": "npx --no parcel build --no-source-maps --no-content-hash --dist-dir build/page public/index.html --public-url=./",
    "build-clean": "npx shx touch build && npx shx rm -r build",
    "_build": "npm run build-clean && npx npm-run-all build:*",
    "publish": "npm publish --access public",
    "serve": "react-scripts start",
    "build": "npm run build-clean && react-scripts build",
    "_TODO_test": "react-scripts test",
    "_eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@wingechr/javascript_package": "file:../javascript_package",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react-scripts": "5.0.1",
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@parcel/transformer-babel": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "parcel": "^2.12.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4"
  }
}
